		"next": "^13.4.4",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-icons": "^4.9.0",
		"sharp": "^0.32.1",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.6.5",
