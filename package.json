		"framer-motion": "^10.16.4",
		"next": "^14.0.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-icons": "^4.11.0",
		"sharp": "^0.32.6",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@commitlint/cli": "^18.2.0",
		"@commitlint/config-conventional": "^18.1.0",
