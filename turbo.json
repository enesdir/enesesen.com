{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["type-check"],
			"outputs": [".next/**", "!.next/cache/**", "public/**"],
			"env": ["NEXT_PUBLIC_FORMSPREE_URL", "NEXT_PUBLIC_CALENDLY_URL", "NEXT_PUBLIC_VERCEL_URL"]
		},
		"lint": {
			"outputs": [],
			"inputs": [".eslintrc.*", "src/**"]
		},
		"type-check": {
			"outputs": ["*.tsbuildinfo"]
		}
	}
}
